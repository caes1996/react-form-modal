{
  "name": "react-form-modal",
  "version": "0.1.4",
  "description": "Modal de formularios reutilizable con Reactstrap y react-datepicker",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup --no-dts",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=17",
    "react-datepicker": ">=7",
    "react-dom": ">=17",
    "react-number-format": ">=5",
    "reactstrap": ">=9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caes1996/react-form-modal.git"
  },
  "bugs": {
    "url": "https://github.com/caes1996/react-form-modal/issues"
  },
  "homepage": "https://github.com/caes1996/react-form-modal#readme",
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-number-format": "^5.4.4",
    "reactstrap": "^9.2.3",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
